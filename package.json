    "chai": "^3.5.0",
    "dc": "1.7.3",
    "d3": "3.5.16",
    "crossfilter": "1.3.12",
    "css-loader": "^0.27.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "jsdom": "^9.12.0",
